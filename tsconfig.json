{

    "compilerOptions": {
        "moduleResolution": "nodenext",
        "checkJs": true,
        "allowJs": true,
        "declaration":true,
        "emitDeclarationOnly": true,
        "declarationMap": true,
        "outDir": "types",
        "noImplicitAny": true,
        "noImplicitReturns": false,
        "esModuleInterop": true
    },
    "include": ["src/**/*", "*.js", "test/test.js"],
    "exclude": ["node_modules", "types"]
}